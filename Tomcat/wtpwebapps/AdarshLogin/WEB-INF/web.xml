  <display-name>AdarshLogin</display-name>
  <welcome-file-list>
    <welcome-file>userlogin.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>loginController</servlet-name>
  	<servlet-class>com.interland.page.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loginController</servlet-name>
  	<url-pattern>/loginController</url-pattern>
  </servlet-mapping>
</web-app>